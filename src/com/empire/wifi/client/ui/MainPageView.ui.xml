<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="main-body">

		<span ui:field="disconnectionText" class="serverdisconnected alert hide"></span>

		<div class="alert alert-success hidden" ui:field="divAlert">
			<i class="icon-ok-sign"/>
			<span ui:field="spnAlertContent"></span>
			<strong ui:field="spnSubject" class="hidden"></strong>
		</div>

		<g:HTMLPanel ui:field="pHeader" styleName="top-header"></g:HTMLPanel>
		<!-- g:HTMLPanel ui:field="pContainer"/ -->

		<!-- Domain Specific View goes here -->
		<g:HTMLPanel styleName="body-container" ui:field="divBody">
			<g:HTMLPanel ui:field="pContainer" styleName="full-page">
			</g:HTMLPanel>
		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder>
